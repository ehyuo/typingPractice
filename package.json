{
  "name": "typingpractice",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "dependencies": {
    "@react-spring/web": "^9.2.4",
    "@reduxjs/toolkit": "^1.6.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "http-proxy-middleware": "^0.19.1",
    "nodemon": "^2.0.9",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-bar": "^0.16.3",
    "react-bars": "^0.1.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-transition-group": "^2.9.0",
    "recharts": "^2.0.10",
    "redux": "^4.1.1",
    "redux-actions": "^2.6.5",
    "styled-components": "^5.3.0",
    "web-vitals": "^1.1.2"
  },
  "peerDependencies": {
    "react-spring": "^8.0.27"
  },
  "scripts": {
    "startElectron": "electron ./public/electron",
    "start": "concurrently \"npm-run-all --parallel start:**\" \"wait-on http://localhost:3000 && electron ./public/electron\"",
    "start:client": "react-scripts start",
    "start:server": "nodemon ./server/app",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dist": "npm run build && electron-builder"
  
  },
  "build": {
    "extends": null,
    "directories": {
      "buildResources": "public"
    },
    "extraMetadata": {
      "main": "dist/main.js"
    },
    "target" : "zip"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "electron": "^14.0.1"
  },
  
  "homepage": "./"
}
